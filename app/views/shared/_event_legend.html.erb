<ul class="event-legend">
  <% if show_public_holiday %>
  <li>
    <i class="icon-flag public-holiday-color"></i>
    <%= t '.public_holiday' %>
  </li>
  <% end %>

  <% time_types.each do |time_type| %>
  <li>
    <%= icon_for_time_type(time_type) %>
    <%= time_type %>
  </li>
  <% end %>
</ul>
